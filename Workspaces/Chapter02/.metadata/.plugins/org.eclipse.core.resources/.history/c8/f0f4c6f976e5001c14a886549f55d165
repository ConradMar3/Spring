package tacos;

import lombok.Data;

@Data
public class Ingredient {
  
  private final String id = "";
  private final String name = "";
  private final Type type = null;
  
  public enum Type {
    WRAP, PROTEIN, VEGGIES, CHEESE, SAUCE
  }

public String getId() {
	return id;
}

public String getName() {
	return name;
}

public Type getType() {
	return type;
}

}
